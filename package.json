{
  "version": "1.0.0",
  "name": "vue-translate-app",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "eslint --ext vue,js,ts",
    "lint:fix": "npm run lint -- --fix",
    "ssr:build": "vue-cli-service ssr:build",
    "ssr:serve": "cross-env VUE_APP_SSR=1 vue-cli-service ssr:serve",
    "ssr:start": "cross-env NODE_ENV=production VUE_APP_SSR=1 vue-cli-service ssr:serve --mode production",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "now-build": "vue-cli-service build"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "axios": "^0.18.0",
    "core-js": "^2.6.5",
    "fetch-jsonp": "^1.1.3",
    "register-service-worker": "^1.6.2",
    "vue": "^2.6.10",
    "vue-axios": "^2.1.4",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.0.3",
    "vue-server-renderer": "^2.6.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@akryum/vue-cli-plugin-ssr": "^0.3.0",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@types/chai": "^4.1.0",
    "@types/mocha": "^5.2.4",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-e2e-cypress": "^3.7.0",
    "@vue/cli-plugin-pwa": "^3.7.0",
    "@vue/cli-plugin-typescript": "^3.7.0",
    "@vue/cli-plugin-unit-mocha": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.1",
    "chai": "^4.1.2",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-vue": "^5.2.2",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.17.0",
    "typescript": "^3.4.3",
    "vue-template-compiler": "^2.5.21"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -E GIT_PARAMS"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "npm run lint:fix",
      "git add"
    ]
  }
}
